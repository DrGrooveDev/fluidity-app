package main

import (
    "log"

    "github.com/fluidity-money/fluidity-app/common/solana"
    "github.com/fluidity-money/fluidity-app/lib/util"
)

func main() {
    wallet, err := solana.WalletFromPrivateKeyBase58(util.GetEnvOrFatal("FLU_WORKER_PRIKEY"))
    if err != nil {
        panic(err)
    }

    log.Printf("%+v", wallet.PublicKey())
}
