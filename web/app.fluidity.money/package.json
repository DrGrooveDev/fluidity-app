{
  "name": "new.fluidity.money",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect ./build/server.js",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "test": "npm-run-all --parallel test:*",
    "test:lint": "eslint --ext .js,.ts,.tsx --fix .",
    "test:style": "prettier -w .",
    "test:unit": "jest --config jest.config.ts --passWithNoTests",
    "graphql": "npm-run-all graphql:generate graphql:codegen",
    "graphql:generate": "gq --introspect https://fluidity.hasura.app/v1/graphql > schema.graphql",
    "graphql:codegen": "relay-compiler"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@fluidity-money/surfing": "^0.0.66",
    "@openzeppelin/contracts": "^4.7.3",
    "@remix-run/react": "^1.7.1",
    "@remix-run/serve": "^1.7.1",
    "@solana/web3.js": "^1.63.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.1",
    "framer-motion": "^7.3.6",
    "graphql-ws": "^5.10.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "toml": "^3.0.0",
    "web3": "^1.8.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.1",
    "@remix-run/node": "^1.7.1",
    "@sentry/react": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "@types/bn.js": "^5.1.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.2",
    "@types/morgan": "^1.9.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/relay-runtime": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "esbuild": "^0.15.10",
    "eslint": "^8.23.1",
    "eslint-plugin-react": "^7.31.8",
    "graphql": "^16.6.0",
    "graphqurl": "^1.0.1",
    "jest": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
